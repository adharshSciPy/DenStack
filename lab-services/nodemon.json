{
  "watch": [
    "controllers/",
    "models/",
    "routes/",
    "utils/",
    "services/",
    "index.js",
    ".env"
  ],
  "ignore": [
    "uploads/**",
    "*.nii",
    "*.nii.gz",
    "*.json",
    "*.dcm",
    "node_modules/**",
    ".git/**",
    "test/**"
  ],
  "ext": "js,json",
  "env": {
    "NODE_ENV": "development"
  },
  "delay": 2500,
  "legacyWatch": false
}